{
  "name": "prisma-generator-pothos-codegen",
  "version": "0.2.3",
  "description": "A prisma generator that generate input types for Pothos. And optionally generate all crud.",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./src/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cauen/prisma-generator-pothos-codegen.git"
  },
  "author": "Emanuel CauÃª",
  "keywords": [
    "pothos",
    "graphql",
    "typescript",
    "prisma",
    "db",
    "postgres",
    "codegen"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "tscheck": "tsc -noEmit",
    "prebuild": "yarn clean",
    "postbuild": "yarn copyfiles ./readme.md ./dist",
    "clean": "rimraf dist",
    "pub": "cd dist && npm publish",
    "format": "eslint --fix ./src"
  },
  "bin": {
    "prisma-generator-pothos-codegen": "./src/bin.js"
  },
  "peerDependencies": {
    "@pothos/core": "^3.17.0",
    "@pothos/plugin-prisma": "^3.27.0"
  },
  "devDependencies": {
    "@pothos/core": "3.17.0",
    "@pothos/plugin-prisma": "3.27.1",
    "@prisma/client": "4.2.1",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.10",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "@typescript-eslint/parser": "5.34.0",
    "copyfiles": "2.4.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "graphql": "16.6.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "prisma": "4.2.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@prisma/generator-helper": "4.2.1",
    "fs-extra": "10.1.0"
  }
}
