{
  "name": "prisma-generator-pothos-codegen",
  "version": "0.1.0",
  "description": "A prisma generator that generate input types for Pothos",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./src/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cauen/prisma-generator-pothos-codegen.git"
  },
  "author": "Emanuel CauÃª",
  "keywords": [
    "pothos",
    "graphql",
    "typescript",
    "prisma",
    "db",
    "postgres",
    "codegen"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prebuild": "yarn clean",
    "postbuild": "yarn copyfiles ./readme.md ./dist",
    "clean": "rimraf dist",
    "pub": "cd dist && npm publish"
  },
  "bin": {
    "prisma-generator-pothos-codegen": "./src/bin.js"
  },
  "devDependencies": {
    "@pothos/core": "^3.12.2",
    "@pothos/plugin-prisma": "^3.13.1",
    "@prisma/client": "3.12",
    "@prisma/sdk": "3.12",
    "@types/fs-extra": "^9.0.13",
    "copyfiles": "^2.4.1",
    "graphql": "^16.5.0",
    "prisma": "3.12",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/jest": "^28.1.4",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5"
  }
}
